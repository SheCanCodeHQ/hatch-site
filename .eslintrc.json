{
    "extends": "airbnb",
    "env": {
      "browser": true
    },
    "parserOptions":  {
      "ecmaVersion": 8,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
      "quotes": [ "error", "double" ]
    },
    "globals": {
    }
}
